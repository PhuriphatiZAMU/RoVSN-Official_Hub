# MongoDB Connection (Replace with your actual credentials)
MONGO_URI=mongodb+srv://YOUR_USERNAME:YOUR_PASSWORD@YOUR_CLUSTER.mongodb.net/YOUR_DATABASE?retryWrites=true&w=majority

# Server Port
PORT=3000

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Admin Credentials
ADMIN_USERNAME=admin
# Generate hash using: POST /api/auth/hash with { "password": "your-password" }
ADMIN_PASSWORD_HASH=
