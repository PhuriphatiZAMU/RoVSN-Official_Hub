{
  "name": "rovsn-official-hub-server",
  "version": "1.0.0",
  "description": "Backend server for ROV SN Tournament 2026 Official Hub with MongoDB Atlas",
  "main": "server.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "install-client": "cd ../client && npm install",
    "build": "npm install && npm run install-client && cd ../client && npm run build",
    "start": "NODE_PATH=./node_modules node server.js",
    "dev": "node server.js"
  },
  "keywords": [
    "rov",
    "tournament",
    "mongodb",
    "express",
    "api"
  ],
  "author": "PhuriphatiZAMU",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/PhuriphatiZAMU/RoVSN-Official_Hub.git"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}